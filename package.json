{
    "name": "gitlab-commander",
    "description": "Apply gitlab actions for multiple projects",
    "version": "0.0.1",
    "author": "JungHun Kim @mnthe",
    "bin": {
        "gitlab-commander": "./bin/run"
    },
    "bugs": "https://github.com/mnthe/gitlab-commander/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.2",
        "axios": "^0.21.1",
        "chalk": "^4.1.0",
        "cli-progress": "^3.9.0",
        "lodash.template": "^4.5.0",
        "micromatch": "^4.0.2",
        "treeify": "^1.1.0",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.15",
        "@types/cli-progress": "^3.9.0",
        "@types/lodash.template": "^4.5.0",
        "@types/micromatch": "^4.0.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.54",
        "@types/treeify": "^1.0.0",
        "chai": "^4.3.0",
        "eslint": "^5.16.0",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.1.0",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.9"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/mnthe/gitlab-commander",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "gitlab-commander",
        "helpClass": "./src/help",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "mnthe/gitlab-commander",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
